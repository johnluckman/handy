    "@react-navigation/stack": "^7.3.6",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "~53.0.12",
    "expo-camera": "~15.0.0",
    "expo-barcode-scanner": "~14.0.0",
    "expo-font": "^13.3.1",
    "expo-linear-gradient": "~14.1.5",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-dotenv": "^3.4.11",
